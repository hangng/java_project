        <activity android:name="com.example.n3333.practice.UploadImageActivity" />
        <activity android:name="com.example.n3333.practice.FragmentActivity" />
        <activity android:name="com.example.n3333.practice.ExpendableActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="testing.com.my"
                    android:pathPattern="/test"
                    android:scheme="http" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.n3333.practice.ProfilePractice" />
        <activity android:name="com.example.n3333.practice.BookLoadingTutorial" />
